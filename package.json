    "chai-exclude": "^3.0.1",
    "chai-spies": "^1.1.0",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^16.4.0",
    "mocha": "^11.7.2",
    "npm-check-updates": "^18.3.0",
    "prettier": "^3.6.2"
  },
  "repository": "github:patkub/auth0-rule-as-action",
  "homepage": "https://github.com/patkub/auth0-rule-as-action",
  "bugs": "https://github.com/patkub/auth0-rule-as-action/issues",
